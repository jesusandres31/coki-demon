{
  "name": "wpp-calendar-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "1": "npm i -g pm2",
    "2": "cd scheduler && npm i && cd .. && npm i",
    "3": "cross-env NODE_ENV=production concurrently \"npx -p typescript tsc\" \"pm2 start dist/src/index.js --name coki-demon-app\"",
    "4": "pm2 start scheduler/index.js --name coki-demon-scheduler",
    "5": "pm2 save",
    "dev": "concurrently \"tsc --watch\" \"nodemon dist/src/index.js\"",
    "start": "pm2 start coki-demon-app",
    "stop": "pm2 stop coki-demon-app",
    "start:all": "pm2 start coki-demon-app && pm2 start coki-demon-scheduler",
    "stop:all": "pm2 stop coki-demon-app && pm2 stop coki-demon-scheduler",
    "deploy": "npm run 1 && npm run 2 && npm run 3 && npm run 4 && npm run 5"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5n",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "googleapis": "^105.0.0",
    "morgan": "^1.10.0",
    "node-adodb": "^5.0.3",
    "node-cron": "^3.0.1",
    "qr-image": "^3.2.0",
    "qrcode-terminal": "^0.12.0",
    "typescript": "^4.7.4",
    "whatsapp-web.js": "^1.21.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node-cron": "^3.0.2",
    "@types/qr-image": "^3.2.5",
    "@types/qrcode-terminal": "^0.12.0",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19"
  }
}
